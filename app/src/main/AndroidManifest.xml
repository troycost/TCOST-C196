<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.wgu.TCost_C196">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TCost_C196">
        <receiver
            android:name="com.wgu.TCost_C196.UI.MyReceiver"
            android:enabled="true"
            android:exported="true"></receiver>

        <activity
            android:name="com.wgu.TCost_C196.UI.AssessmentDetails"
            android:label="Assessment Details"
            android:windowSoftInputMode="adjustPan" />
        <activity
            android:name="com.wgu.TCost_C196.UI.AssessmentList"
            android:label="Course Details and Assessment List"
            android:windowSoftInputMode="adjustPan" />
        <activity
            android:name="com.wgu.TCost_C196.UI.CourseList"
            android:label="Term Details and Course List"
            android:windowSoftInputMode="adjustPan" />
        <activity
            android:name="com.wgu.TCost_C196.UI.TermList"
            android:label="Term List"
            android:windowSoftInputMode="adjustPan" />
        <activity android:name="com.wgu.TCost_C196.UI.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"
                    android:windowSoftInputMode="adjustPan" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>
